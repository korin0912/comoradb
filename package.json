{
  "name": "comoradb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "run-p serve:*",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "daily-update": "run-s daily-update:{build,git-push}",
    "daily-update:build": "npm run build",
    "daily-update:git-push": "git add . && git commit -m \"daily update\" && git push",
    "serve:daily-update-server": "node .\\tools\\daily_update_server.js",
    "serve:json-edit-server": "node .\\tools\\json_edit_server.js",
    "serve:vue-cli-service": "vue-cli-service serve"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.9",
    "axios": "^0.21.4",
    "core-js": "^3.24.1",
    "vue": "^3.2.37",
    "vue-burger-menu": "^2.0.5",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.37",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.20.0",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
